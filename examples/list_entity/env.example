# DeltaStream Connection Configuration
# Copy this file to .env and fill in your actual values

# Required Environment Variables
DELTASTREAM_TOKEN=your-api-token-here
DELTASTREAM_ORG_ID=your-organization-id-here

# Optional Configuration
DELTASTREAM_SERVER_URL=https://your-deltastream-host:443
DELTASTREAM_DATABASE_NAME=your-default-database
DELTASTREAM_SCHEMA_NAME=your-default-schema
DELTASTREAM_TIMEZONE=UTC
DELTASTREAM_ROLE_NAME=your-role
DELTASTREAM_STORE_NAME=your-default-store
DELTASTREAM_COMPUTE_POOL_NAME=your-default-compute-pool 
